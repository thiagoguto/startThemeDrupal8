<div class="uk-offcanvas-content">
  <div uk-sticky="media: 960" class="uk-navbar-container uk-sticky uk-sticky-fixed uk-active uk-sticky-below">
    <div class="uk-container uk-container-expand">
      <nav class="uk-navbar">
        <div class="uk-navbar-left">
          <div class="uk-navbar-item uk-logo">
          {{ page.header.tema_branding }}
          </div>
        </div>
        <div class="uk-navbar-right uk-visible@m">
          {{ page.header.tema_main_menu }}
        </div>
        <div class="uk-navbar-right uk-hidden@m">
          <div class="uk-navbar-item ">
          <a uk-navbar-toggle-icon="" href="#offcanvas" uk-toggle="" class="uk-navbar-toggle uk-hidden@m uk-navbar-toggle-icon uk-icon"></a>
          </div>
        </div>
      </nav>
    </div>
  </div>
  {% if page.banner == true %}
  <h1>banner verdadeiro</h1>
    {{ page.banner }}
  {% endif %}
<div class="uk-section">
  <div class="uk-container uk-container-large">
  {% if page.help %}
    {{ page.help }}
  {% endif %}
    {{ page.content }}
  </div>
</div>

<footer class="uk-section uk-section-secondary uk-light">
  <div class="uk-container uk-container-large">
    {{ page.footer }}
  </div>
</footer>
</div>
<div id="offcanvas" uk-offcanvas="mode: push; overlay: true" class="uk-offcanvas">
  <div class="uk-offcanvas-bar">
    <div class="uk-panel">
      {{ page.mobile.mobile_main_navigation }}
      <ul class="uk-nav uk-nav-default tm-nav">
      </ul>
    </div>
  </div>
</div>
